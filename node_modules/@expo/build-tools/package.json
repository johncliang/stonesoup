{
  "name": "@expo/build-tools",
  "version": "0.1.0-alpha.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "yarn watch",
    "watch": "yarn compile --watch --preserveWatchOutput",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist tsconfig.tsbuildinfo",
    "compile": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test:unit": "yarn run jest",
    "test:unit:watch": "yarn test:unit --watch",
    "test:unit:ci-linux": "yarn test:unit -c jest.ci-linux.config.js",
    "test:unit:ci-macos": "yarn test:unit -c jest.ci-macos.config.js"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.5.0",
    "@hapi/boom": "^7.4.3",
    "@hapi/joi": "^16.1.2",
    "@types/bunyan": "^1.8.6",
    "@types/hapi__joi": "^15.0.4",
    "bunyan": "^1.8.12",
    "envsub": "^3.0.9",
    "fs-extra": "^8.1.0",
    "got": "^9.6.0",
    "lodash": "^4.17.15",
    "node-forge": "^0.9.1",
    "plist": "^3.0.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/fs-extra": "^8.0.1",
    "@types/got": "^9.6.7",
    "@types/hapi__boom": "^7.4.1",
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.136",
    "@types/node-forge": "^0.8.6",
    "@types/plist": "^3.0.2",
    "@types/uuid": "^3.4.5",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.2"
  }
}
